includes:
    - phpstan-baseline.neon

parameters:
    level: max
    paths:
        - bin/
        - config/
        - migrations/
        - public/
        - src/
        - tests/

    symfony:
        container_xml_path: %rootDir%/../../../var/cache/dev/App_KernelDevDebugContainer.xml
    scanDirectories:
       - var/cache/dev/Symfony/Config
